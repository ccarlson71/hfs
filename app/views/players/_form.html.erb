<%= semantic_form_for @player do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :lifetime_member, :onchange => "check_selected_value('lifetime_member');" %>
    <%= f.input :dues_paid %>
    <%= f.input :dues_paid_through, :as => :datepicker %>
  <% end %>
  <%= f.buttons %>
<% end %>
